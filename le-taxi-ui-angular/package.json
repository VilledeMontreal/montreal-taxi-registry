{
  "name": "le-taxi-front-angular",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "serve:local": "ng serve --environment=local --watch",
    "serve:dev": "ng serve --environment=dev --watch",
    "serve:accept": "ng serve --environment=accept --watch",
    "start": "node server.js",
    "build": "ng build",
    "build:dev": "ng build --env=dev --sourcemaps=true --prod -aot=false",
    "build:accept": "ng build --env=accept --prod -aot=false",
    "build:prod": "ng build --env=prod --prod -aot=false",
    "test": "ng test --single-run",
    "lint": "prettier -l \"**/*.ts\" && ng lint",
    "lint-fix": "prettier \"**/*.ts\" --write && ng lint --fix",
    "e2e": "ng e2e",
    "sonar": "sh ./sonar.sh"
  },
  "private": true,
  "devDependencies": {
    "@angular/cli": "1.7.4",
    "@angular/compiler-cli": "5.2.11",
    "@angular/language-service": "4.0.0",
    "@types/jasmine": "3.4.0",
    "@types/leaflet": "^1.0.69",
    "@types/node": "6.0.60",
    "@types/superagent": "4.1.1",
    "codelyzer": "3.0.1",
    "jasmine-core": "2.6.2",
    "jasmine-spec-reporter": "4.1.0",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.1.1",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.2.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "prettier": "2.2.1",
    "protractor": "5.1.2",
    "ts-node": "3.0.4",
    "tslint": "5.5.0",
    "typescript": "2.4.2"
  },
  "dependencies": {
    "@angular/animations": "5.2.11",
    "@angular/cdk": "2.0.0-beta.8",
    "@angular/common": "5.2.11",
    "@angular/compiler": "5.2.11",
    "@angular/core": "11.0.5",
    "@angular/forms": "5.2.11",
    "@angular/http": "5.2.11",
    "@angular/material": "2.0.0-beta.8",
    "@angular/platform-browser": "5.2.11",
    "@angular/platform-browser-dynamic": "5.2.11",
    "@angular/platform-server": "5.2.11",
    "@angular/router": "5.2.11",
    "core-js": "2.4.1",
    "express": "4.16.4",
    "express-prom-bundle": "3.3.0",
    "rxjs": "5.5.12",
    "sonarqube-scanner": "^2.7.0",
    "superagent": "5.1.0",
    "zone.js": "0.8.4"
  }
}
